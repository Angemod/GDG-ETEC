<p id="notice"><%= notice %></p>

<h1>Registers</h1>

<table>
  <thead>
    <tr>
      <th>Registration</th>
      <th>Name</th>
      <th>Course</th>
      <th>Module</th>
      <th>Date of registration</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @registers.each do |register| %>
      <tr>
        <td><%= register.registration %></td>
        <td><%= register.name %></td>
        <td><%= register.course %></td>
        <td><%= register.module %></td>
        <td><%= register.date_of_registration %></td>
        <td><%= link_to 'Show', register %></td>
        <td><%= link_to 'Edit', edit_register_path(register) %></td>
        <td><%= link_to 'Destroy', register, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


